        <rdfs:label xml:lang="it">VideoGioco</rdfs:label>
    </owl:Class>
    


    <!-- http://www.narrative-universes.org/ontology#Weapon -->

    <owl:Class rdf:about="http://www.narrative-universes.org/ontology#Weapon">
