        <rdfs:comment xml:lang="it">Una serie televisiva con episodi multipli</rdfs:comment>
        <rdfs:label xml:lang="it">Serie TV</rdfs:label>
    </owl:Class>
    


    <!-- http://www.narrative-universes.org/ontology#TheatricalPlay -->

    <owl:Class rdf:about="http://www.narrative-universes.org/ontology#TheatricalPlay">
