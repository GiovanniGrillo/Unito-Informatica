<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/aless/ontologies/2026/0/untitled-ontology-3/"
     xml:base="http://www.semanticweb.org/aless/ontologies/2026/0/untitled-ontology-3/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ont="http://www.co-ode.org/ontologies/ont.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:ontology="http://www.narrative-universes.org/ontology#">
    <owl:Ontology rdf:about="http://www.narrative-universes.org/ontology">
        <owl:versionIRI rdf:resource="http://www.narrative-universes.org/ontology/1.0"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <dc:creator>Alessandro Olivero</dc:creator>
        <dc:creator>Giovanni Grillo</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2026-01-10T00:00:00</dc:date>
        <dc:description xml:lang="en">An ontology for representing crossmedia narrative universes including characters, locations, objects, works, and narrative roles</dc:description>
        <dc:title xml:lang="it">Ontologia degli universi narrativi</dc:title>
        <vann:preferredNamespacePrefix>&quot;nuo&quot;</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>&quot;http://www.narrative-universes.org/ontology#&quot;</vann:preferredNamespaceUri>
        <rdfs:label xml:lang="en">Narrative Universes Ontology</rdfs:label>
    </owl:Ontology>
    


