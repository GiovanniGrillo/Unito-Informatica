    <!-- http://www.narrative-universes.org/ontology#adaptationOf -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#adaptationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:comment xml:lang="it">Collega un&apos;opera (tipicamente film) all&apos;opera originale (libro) da cui è adattata</rdfs:comment>
        <rdfs:label xml:lang="it">adattamento di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#alliedWith -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#alliedWith">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:comment xml:lang="it">Collega due personaggi che sono alleati o compagni</rdfs:comment>
        <rdfs:label xml:lang="it">alleato con</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#appearsIn -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#appearsIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.narrative-universes.org/ontology#features"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:comment xml:lang="it">Collega un personaggio a un&apos;opera narrativa in cui appare</rdfs:comment>
        <rdfs:label xml:lang="it">appare in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#belongsToUniverse -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#belongsToUniverse">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#NarrativeUniverse"/>
        <rdfs:comment xml:lang="it">Collega un&apos;entità (personaggio, luogo, oggetto, creatura) all&apos;universo narrativo a cui appartiene</rdfs:comment>
        <rdfs:label xml:lang="it">appartiene a universo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#enemyOf -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#enemyOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:comment xml:lang="it">Collega due personaggi che sono nemici dichiarati</rdfs:comment>
        <rdfs:label xml:lang="it">nemico di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#features -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#features">
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:comment xml:lang="it">Indica quali personaggi appaiono in un&apos;opera narrativa</rdfs:comment>
        <rdfs:label xml:lang="it">presente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#grantsAbility -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#grantsAbility">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Ability"/>
        <rdfs:comment xml:lang="it">Collega un artefatto di potere all&apos;abilità che concede</rdfs:comment>
        <rdfs:label xml:lang="it">concede abilità</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hasAbility -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#hasAbility">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Ability"/>
        <rdfs:comment xml:lang="it">Collega un personaggio a un&apos;abilità che possiede naturalmente (senza artefatti)</rdfs:comment>
        <rdfs:label xml:lang="it">ha abilità</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hasMainProtagonist -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#hasMainProtagonist">
        <rdfs:subPropertyOf rdf:resource="http://www.narrative-universes.org/ontology#features"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Protagonist"/>
        <rdfs:comment xml:lang="it">Collega un&apos;opera al suo protagonista principale (uno solo per opera)</rdfs:comment>
        <rdfs:label xml:lang="it">ha protagonista principale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hasMember -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#hasMember">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.narrative-universes.org/ontology#memberOf"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Organization"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:comment xml:lang="it">Indica i membri di un&apos;organizzazione</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hasOriginType -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#hasOriginType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#HybridCharacter"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#OriginType"/>
        <rdfs:comment xml:lang="it">Collega un personaggio ibrido ai suoi tipi di origine (es: umano + divino)</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di origine</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hasPowerTypeEnum -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#hasPowerTypeEnum">
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Object"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#PowerType"/>
        <rdfs:comment xml:lang="it">Collega un oggetto magico al suo tipo di potere tramite la classe enumerata PowerType</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di potere</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hosts -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#hosts">
        <owl:inverseOf rdf:resource="http://www.narrative-universes.org/ontology#locatedAt"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Location"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Organization"/>
        <rdfs:comment xml:lang="it">Indica quali organizzazioni si trovano in un luogo</rdfs:comment>
        <rdfs:label xml:lang="it">ospita</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#indirectlyMentors -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#indirectlyMentors">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.narrative-universes.org/ontology#mentors"/>
            <rdf:Description rdf:about="http://www.narrative-universes.org/ontology#alliedWith"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Indica una relazione di mentoring indiretta attraverso alleanze</rdfs:comment>
        <rdfs:label xml:lang="it">fa da mentore indirettamente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#indirectlySetIn -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#indirectlySetIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.narrative-universes.org/ontology#partOf"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.narrative-universes.org/ontology#setIn"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Usata per rappresentare legami indiretti tra entità narrative e ambientazioni</rdfs:comment>
        <rdfs:label xml:lang="it">Impostato indirettamente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#locatedAt -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#locatedAt">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Organization"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Location"/>
        <rdfs:comment xml:lang="it">Collega un&apos;organizzazione al luogo fisico dove si trova</rdfs:comment>
        <rdfs:label xml:lang="it">situato in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#memberOf -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#memberOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Organization"/>
        <rdfs:comment xml:lang="it">Collega un personaggio a un&apos;organizzazione di cui fa parte</rdfs:comment>
        <rdfs:label xml:lang="it">membro di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#mentors -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#mentors">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:comment xml:lang="it">Collega un personaggio mentore al personaggio che guida e insegna</rdfs:comment>
        <rdfs:label xml:lang="it">fa da mentore a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#partOf -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#partOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Location"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Location"/>
        <rdfs:comment xml:lang="it">Collega un luogo a un luogo più grande che lo contiene</rdfs:comment>
        <rdfs:label xml:lang="it">parte di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#possesses -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#possesses">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Object"/>
        <rdfs:comment xml:lang="it">Collega un personaggio a un oggetto che possiede o controlla</rdfs:comment>
        <rdfs:label xml:lang="it">possiede</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#prequelOf -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#prequelOf">
        <owl:inverseOf rdf:resource="http://www.narrative-universes.org/ontology#sequelOf"/>
        <rdfs:comment xml:lang="it">Indica che un&apos;opera narrativa è il prequel (storia precedente) di un&apos;altra opera</rdfs:comment>
        <rdfs:label xml:lang="it">prequel di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#sequelOf -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#sequelOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:comment xml:lang="it">Collega un&apos;opera all&apos;opera precedente di cui è il seguito</rdfs:comment>
        <rdfs:label xml:lang="it">seguito di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.narrative-universes.org/ontology#setIn -->

    <owl:ObjectProperty rdf:about="http://www.narrative-universes.org/ontology#setIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:range rdf:resource="http://www.narrative-universes.org/ontology#Location"/>
        <rdfs:comment xml:lang="it">Collega un&apos;opera narrativa al luogo in cui è ambientata</rdfs:comment>
        <rdfs:label xml:lang="it">ambientato in</rdfs:label>
    </owl:ObjectProperty>
    


