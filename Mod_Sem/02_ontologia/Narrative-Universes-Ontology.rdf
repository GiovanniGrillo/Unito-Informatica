     -->

    


    <!-- http://www.co-ode.org/ontologies/ont.owl#hasSeasons -->

    <owl:DatatypeProperty rdf:about="http://www.co-ode.org/ontologies/ont.owl#hasSeasons">
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#NarrativeWork"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="it">Il numero di stagioni che una serie televisiva possiede</rdfs:comment>
        <rdfs:label xml:lang="it">ha stagioni</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.narrative-universes.org/ontology#birthYear -->

    <owl:DatatypeProperty rdf:about="http://www.narrative-universes.org/ontology#birthYear">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Character"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="it">L&apos;anno di nascita di un personaggio</rdfs:comment>
        <rdfs:label xml:lang="it">anno di nascita</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.narrative-universes.org/ontology#canBeDestroyed -->

    <owl:DatatypeProperty rdf:about="http://www.narrative-universes.org/ontology#canBeDestroyed">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="it">Indica se un artefatto può essere distrutto</rdfs:comment>
        <rdfs:label xml:lang="it">può essere distrutto</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.narrative-universes.org/ontology#description -->

    <owl:DatatypeProperty rdf:about="http://www.narrative-universes.org/ontology#description">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Una descrizione testuale dell&apos;entità</rdfs:comment>
        <rdfs:label xml:lang="it">descrizione</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.narrative-universes.org/ontology#hasNarrativeFunction -->

    <owl:DatatypeProperty rdf:about="http://www.narrative-universes.org/ontology#hasNarrativeFunction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">La funzione narrativa di un elemento (es: Transizione, Rifugio, Conflitto)</rdfs:comment>
        <rdfs:label xml:lang="it">ha funzione narrativa</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.narrative-universes.org/ontology#isOngoing -->

    <owl:DatatypeProperty rdf:about="http://www.narrative-universes.org/ontology#isOngoing">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.narrative-universes.org/ontology#TVSeries"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="it">Indica se una serie TV è ancora in produzione</rdfs:comment>
        <rdfs:label xml:lang="it">in corso</rdfs:label>
    </owl:DatatypeProperty>
    


