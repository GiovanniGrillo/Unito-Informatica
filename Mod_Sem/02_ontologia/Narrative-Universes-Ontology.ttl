@prefix : <http://www.narrative-universes.org/ontology#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <http://www.narrative-universes.org/ontology#> .

<http://www.narrative-universes.org/ontology> rdf:type owl:Ontology ;
                                               owl:versionIRI <http://www.narrative-universes.org/ontology/1.0> ;
                                               owl:imports <http://www.w3.org/2004/02/skos/core> ;
                                               <http://purl.org/dc/elements/1.1/creator> "Alessandro Olivero" ,
                                                                                         "Giovanni Grillo" ;
                                               <http://purl.org/dc/elements/1.1/date> "2026-01-10T00:00:00"^^xsd:dateTime ;
                                               <http://purl.org/dc/elements/1.1/description> "An ontology for representing crossmedia narrative universes including characters, locations, objects, works, and narrative roles"@en ;
                                               <http://purl.org/dc/elements/1.1/title> "Ontologia degli universi narrativi"@it ;
                                               <http://purl.org/vocab/vann/preferredNamespacePrefix> "\"nuo\"" ;
                                               <http://purl.org/vocab/vann/preferredNamespaceUri> "\"http://www.narrative-universes.org/ontology#\"" ;
                                               rdfs:label "Narrative Universes Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#IndirectlyAllyWith
<http://www.co-ode.org/ontologies/ont.owl#IndirectlyAllyWith> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf :alliedWith ;
                                                              rdf:type owl:SymmetricProperty ;
                                                              rdfs:domain :Character ;
                                                              rdfs:range :Character ;
                                                              rdfs:comment "Relazione di alleanza indiretta tra due personaggi"@it ;
                                                              rdfs:label "IndirettamenteAlleatoCon"@it .


###  http://www.narrative-universes.org/ontology#adaptationOf
:adaptationOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :NarrativeWork ;
              rdfs:range :NarrativeWork ;
              rdfs:comment "Collega un'opera (tipicamente film) all'opera originale (libro) da cui è adattata"@it ;
              rdfs:label "adattamento di"@it .


###  http://www.narrative-universes.org/ontology#alliedWith
:alliedWith rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:SymmetricProperty ;
            rdfs:domain :Character ;
            rdfs:range :Character ;
            rdfs:comment "Collega due personaggi che sono alleati o compagni"@it ;
            rdfs:label "AlleatoCon"@it .


###  http://www.narrative-universes.org/ontology#appearsIn
:appearsIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :features ;
           rdfs:domain :Character ;
           rdfs:range :NarrativeWork ;
           rdfs:comment "Collega un personaggio a un'opera narrativa in cui appare"@it ;
           rdfs:label "appare in"@it .


###  http://www.narrative-universes.org/ontology#belongsToUniverse
:belongsToUniverse rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain owl:Thing ;
                   rdfs:range :NarrativeUniverse ;
                   rdfs:comment "Collega un'entità (personaggio, luogo, oggetto, creatura) all'universo narrativo a cui appartiene"@it ;
                   rdfs:label "appartiene a universo"@it .


###  http://www.narrative-universes.org/ontology#enemyOf
:enemyOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:SymmetricProperty ;
         rdfs:domain :Character ;
         rdfs:range :Character ;
         rdfs:comment "Collega due personaggi che sono nemici dichiarati"@it ;
         rdfs:label "nemico di"@it .


###  http://www.narrative-universes.org/ontology#features
:features rdf:type owl:ObjectProperty ;
          rdfs:domain :NarrativeWork ;
          rdfs:range :Character ;
          rdfs:comment "Indica quali personaggi appaiono in un'opera narrativa"@it ;
          rdfs:label "presente"@it .


###  http://www.narrative-universes.org/ontology#grantsAbility
:grantsAbility rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:range :Ability ;
               rdfs:comment "Collega un artefatto di potere all'abilità che concede"@it ;
               rdfs:label "concede abilità"@it .


###  http://www.narrative-universes.org/ontology#hasAbility
:hasAbility rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Character ;
            rdfs:range :Ability ;
            rdfs:comment "Collega un personaggio a un'abilità che possiede naturalmente (senza artefatti)"@it ;
            rdfs:label "ha abilità"@it .


###  http://www.narrative-universes.org/ontology#hasMainProtagonist
:hasMainProtagonist rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :features ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :NarrativeWork ;
                    rdfs:range :Character ,
                               :Protagonist ;
                    rdfs:comment "Collega un'opera al suo protagonista principale (uno solo per opera)"@it ;
                    rdfs:label "ha protagonista principale"@it .


###  http://www.narrative-universes.org/ontology#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :memberOf ;
           rdfs:domain :Organization ;
           rdfs:range :Character ;
           rdfs:comment "Indica i membri di un'organizzazione"@it .


###  http://www.narrative-universes.org/ontology#hasOriginType
:hasOriginType rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Character ,
                           :HybridCharacter ,
                           [ owl:intersectionOf ( :Character
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasOriginType ;
                                                    owl:minCardinality "2"^^xsd:nonNegativeInteger
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasOriginType ;
                             owl:minCardinality "2"^^xsd:nonNegativeInteger
                           ] ;
               rdfs:range :OriginType ,
                          [ rdf:type owl:Class ;
                            owl:oneOf ( <http://www.co-ode.org/ontologies/ont.owl#ElvenOrigin>
                                        <http://www.co-ode.org/ontologies/ont.owl#GiantOrigin>
                                        <http://www.co-ode.org/ontologies/ont.owl#HumanOrigin>
                                        <http://www.co-ode.org/ontologies/ont.owl#SpiritOrigin>
                                        :DivineOrigin
                                      )
                          ] ;
               rdfs:comment "Collega un personaggio ibrido ai suoi tipi di origine (es: umano + divino)"@it ;
               rdfs:label "ha tipo di origine"@it .


###  http://www.narrative-universes.org/ontology#hasPowerTypeEnum
:hasPowerTypeEnum rdf:type owl:ObjectProperty ;
                  rdfs:domain :Object ;
                  rdfs:range :PowerType ,
                             [ rdf:type owl:Class ;
                               owl:oneOf ( <http://www.co-ode.org/ontologies/ont.owl#OrdinaryPower>
                                           :CorruptivePower
                                           :DivinePower
                                           :MagicalPower
                                           :SymbolicPower
                                         )
                             ] ;
                  rdfs:comment "Collega un oggetto magico al suo tipo di potere tramite la classe enumerata PowerType"@it ;
                  rdfs:label "ha tipo di potere"@it .


###  http://www.narrative-universes.org/ontology#hosts
:hosts rdf:type owl:ObjectProperty ;
       owl:inverseOf :locatedAt ;
       rdfs:domain :Location ;
       rdfs:range :Organization ;
       rdfs:comment "Indica quali organizzazioni si trovano in un luogo"@it ;
       rdfs:label "ospita"@it .


###  http://www.narrative-universes.org/ontology#indirectlyMentors
:indirectlyMentors rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :Character ;
                   rdfs:range :Character ;
                   owl:propertyChainAxiom ( :mentors
                                            :alliedWith
                                          ) ;
                   rdfs:comment "Indica una relazione di mentoring indiretta attraverso alleanze"@it ;
                   rdfs:label "fa da mentore indirettamente"@it .


###  http://www.narrative-universes.org/ontology#indirectlySetIn
:indirectlySetIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:propertyChainAxiom ( :partOf
                                        ) ,
                                        ( :setIn
                                        ) ;
                 rdfs:comment "Usata per rappresentare legami indiretti tra entità narrative e ambientazioni"@it ;
                 rdfs:label "Impostato indirettamente"@it .


###  http://www.narrative-universes.org/ontology#locatedAt
:locatedAt rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Organization ;
           rdfs:range :Location ;
           rdfs:comment "Collega un'organizzazione al luogo fisico dove si trova"@it ;
           rdfs:label "situato in"@it .


###  http://www.narrative-universes.org/ontology#memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Character ;
          rdfs:range :Organization ;
          rdfs:comment "Collega un personaggio a un'organizzazione di cui fa parte"@it ;
          rdfs:label "membro di"@it .


###  http://www.narrative-universes.org/ontology#mentors
:mentors rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Character ;
         rdfs:range :Character ;
         rdfs:comment "Collega un personaggio mentore al personaggio che guida e insegna"@it ;
         rdfs:label "fa da mentore a"@it .


###  http://www.narrative-universes.org/ontology#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:TransitiveProperty ;
        rdfs:domain :Location ;
        rdfs:range :Location ;
        rdfs:comment "Collega un luogo a un luogo più grande che lo contiene"@it ;
        rdfs:label "parte di"@it .


###  http://www.narrative-universes.org/ontology#possesses
:possesses rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Character ;
           rdfs:range :Object ;
           rdfs:comment "Collega un personaggio a un oggetto che possiede o controlla"@it ;
           rdfs:label "possiede"@it .


###  http://www.narrative-universes.org/ontology#prequelOf
:prequelOf rdf:type owl:ObjectProperty ;
           owl:inverseOf :sequelOf ;
           rdfs:comment "Indica che un'opera narrativa è il prequel (storia precedente) di un'altra opera"@it ;
           rdfs:label "prequel di"@it .


###  http://www.narrative-universes.org/ontology#sequelOf
:sequelOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :NarrativeWork ;
          rdfs:range :NarrativeWork ;
          rdfs:comment "Collega un'opera all'opera precedente di cui è il seguito"@it ;
          rdfs:label "seguito di"@it .


###  http://www.narrative-universes.org/ontology#setIn
:setIn rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       rdfs:domain :NarrativeWork ;
       rdfs:range :Location ;
       rdfs:comment "Collega un'opera narrativa al luogo in cui è ambientata"@it ;
       rdfs:label "ambientato in"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#hasSeasons
<http://www.co-ode.org/ontologies/ont.owl#hasSeasons> rdf:type owl:DatatypeProperty ;
                                                      rdfs:domain :NarrativeWork ;
                                                      rdfs:range xsd:integer ;
                                                      rdfs:comment "Il numero di stagioni che una serie televisiva possiede"@it ;
                                                      rdfs:label "ha stagioni"@it .


###  http://www.narrative-universes.org/ontology#birthYear
:birthYear rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Character ;
           rdfs:range xsd:integer ;
           rdfs:comment "L'anno di nascita di un personaggio"@it ;
           rdfs:label "AnnoDiNascita"@it .


###  http://www.narrative-universes.org/ontology#canBeDestroyed
:canBeDestroyed rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:range xsd:boolean ;
                rdfs:comment "Indica se un artefatto può essere distrutto"@it ;
                rdfs:label "può essere distrutto"@it .


###  http://www.narrative-universes.org/ontology#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:string ;
             rdfs:comment "Una descrizione testuale dell'entità"@it ;
             rdfs:label "descrizione"@it .


###  http://www.narrative-universes.org/ontology#hasNarrativeFunction
:hasNarrativeFunction rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain :Location ;
                      rdfs:range xsd:string ;
                      rdfs:comment "La funzione narrativa di un elemento (es: Transizione, Rifugio, Conflitto)"@it ;
                      rdfs:label "ha funzione narrativa"@it .


###  http://www.narrative-universes.org/ontology#isOngoing
:isOngoing rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :NarrativeWork ,
                       :TVSeries ,
                       [ owl:intersectionOf ( :NarrativeWork
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.co-ode.org/ontologies/ont.owl#hasSeasons> ;
                                                owl:someValuesFrom xsd:integer
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.co-ode.org/ontologies/ont.owl#hasSeasons> ;
                         owl:someValuesFrom xsd:integer
                       ] ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Indica se una serie TV è ancora in produzione"@it ;
           rdfs:label "in corso"@it .


###  http://www.narrative-universes.org/ontology#numberOfPages
:numberOfPages rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Book ,
                           :NarrativeWork ,
                           [ owl:intersectionOf ( :NarrativeWork
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :numberOfPages ;
                                                    owl:someValuesFrom xsd:integer
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :numberOfPages ;
                             owl:someValuesFrom xsd:integer
                           ] ;
               rdfs:range xsd:integer ;
               rdfs:comment "Il numero di pagine di un libro"@it ;
               rdfs:label "numero di pagine"@it .


###  http://www.narrative-universes.org/ontology#publicationYear
:publicationYear rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :NarrativeWork ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "L'anno in cui un'opera è stata pubblicata"@it ;
                 rdfs:label "AnnoDiPubblicazione"@it .


###  http://www.narrative-universes.org/ontology#rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :NarrativeWork ;
        rdfs:range xsd:float ;
        rdfs:comment "La valutazione media di un'opera su scala 0-10"@it ;
        rdfs:label "valutazione"@it .


###  http://www.narrative-universes.org/ontology#runtime
:runtime rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain :NarrativeWork ;
         rdfs:range xsd:integer ;
         rdfs:comment "La durata in minuti di un film o episodio"@it ;
         rdfs:label "durata"@it .


###  http://www.narrative-universes.org/ontology#title
:title rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:domain :NarrativeWork ;
       rdfs:range xsd:string ;
       rdfs:comment "Il titolo di un'opera narrativa"@it ;
       rdfs:label "titolo"@it .


#################################################################
#    Classes
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#AncientCharacter
<http://www.co-ode.org/ontologies/ont.owl#AncientCharacter> rdf:type owl:Class ;
                                                            rdfs:subClassOf :Character ;
                                                            rdfs:comment "Personaggio nato prima del 1900"@it ;
                                                            rdfs:label "PersonaggioAntico"@it .


###  http://www.co-ode.org/ontologies/ont.owl#HighlyRatedWork
<http://www.co-ode.org/ontologies/ont.owl#HighlyRatedWork> rdf:type owl:Class ;
                                                           rdfs:subClassOf :NarrativeWork ;
                                                           rdfs:comment "Opera con valutazione superiore a 9.0f"@it ;
                                                           rdfs:label "OperaMaggiore"@it .


###  http://www.co-ode.org/ontologies/ont.owl#ModernWork
<http://www.co-ode.org/ontologies/ont.owl#ModernWork> rdf:type owl:Class ;
                                                      rdfs:subClassOf :NarrativeWork ;
                                                      rdfs:comment "Opera pubblicata dopo il 2000"@it ;
                                                      rdfs:label "OperaModerna"@it .


###  http://www.co-ode.org/ontologies/ont.owl#PowerArtifact
<http://www.co-ode.org/ontologies/ont.owl#PowerArtifact> rdf:type owl:Class ;
                                                         owl:equivalentClass [ owl:intersectionOf ( :Object
                                                                                                    [ rdf:type owl:Restriction ;
                                                                                                      owl:onProperty :grantsAbility ;
                                                                                                      owl:someValuesFrom :Ability
                                                                                                    ]
                                                                                                  ) ;
                                                                               rdf:type owl:Class
                                                                             ] ;
                                                         rdfs:subClassOf :Object ,
                                                                         [ owl:intersectionOf ( :Object
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty :grantsAbility ;
                                                                                                  owl:someValuesFrom :Ability
                                                                                                ]
                                                                                              ) ;
                                                                           rdf:type owl:Class
                                                                         ] ;
                                                         owl:disjointWith :Weapon ;
                                                         rdfs:comment "Un oggetto magico di grande potere, non necessariamente un'arma da combattimento. Include anelli, mantelli, bacchette e altri artefatti leggendari"@it ;
                                                         rdfs:label "ArtefattoDiPotere"@it .


###  http://www.co-ode.org/ontologies/ont.owl#PowerfulProtagonist
<http://www.co-ode.org/ontologies/ont.owl#PowerfulProtagonist> rdf:type owl:Class ;
                                                               rdfs:subClassOf :Character ,
                                                                               :Protagonist ;
                                                               rdfs:comment "Protagonista che possiede un Artefatto di Potere"@it ;
                                                               rdfs:label "ProtagonistaPotente"@it .


###  http://www.narrative-universes.org/ontology#Ability
:Ability rdf:type owl:Class ;
         rdfs:comment "Un'abilità o potere che può essere concesso da un artefatto o posseduto da un personaggio"@it ;
         rdfs:label "Abilità"@it .


###  http://www.narrative-universes.org/ontology#Alleato
:Alleato rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Character
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :alliedWith ;
                                                      owl:someValuesFrom :Character
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf [ owl:intersectionOf ( :Character
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :alliedWith ;
                                                  owl:someValuesFrom :Character
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
         rdfs:comment "Amico o compagno che aiuta il protagonista"@it ;
         rdfs:label "Alleato"@it .


###  http://www.narrative-universes.org/ontology#Antagonist
:Antagonist rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Character
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :enemyOf ;
                                                         owl:someValuesFrom :Protagonist
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf [ owl:intersectionOf ( :Character
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :enemyOf ;
                                                     owl:someValuesFrom :Protagonist
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
            owl:disjointWith :Protagonist ;
            rdfs:comment "Villain principale o forza oppositrice"@it ;
            rdfs:label "Antagonista"@it .


###  http://www.narrative-universes.org/ontology#Book
:Book rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :NarrativeWork
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :numberOfPages ;
                                                   owl:someValuesFrom xsd:integer
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :NarrativeWork ,
                      [ owl:intersectionOf ( :NarrativeWork
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :numberOfPages ;
                                               owl:someValuesFrom xsd:integer
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] ;
      rdfs:comment "Un'opera letteraria scritta, tipicamente un romanzo o una serie di libri"@it ;
      rdfs:label "Libro"@it .


###  http://www.narrative-universes.org/ontology#Character
:Character rdf:type owl:Class ;
           rdfs:comment "Un personaggio fittizio che appare in una o più opere narrative"@it ;
           rdfs:label "Personaggio"@it ;
           skos:closeMatch "http://www.wikidata.org/entity/Q95074" .


###  http://www.narrative-universes.org/ontology#DangerZone
:DangerZone rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Location
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasNarrativeFunction ;
                                                         owl:hasValue "Conflitto"
                                                       ]
                                                     ) ;
