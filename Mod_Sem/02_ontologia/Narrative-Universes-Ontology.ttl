@prefix : <http://www.semanticweb.org/aless/ontologies/2026/0/untitled-ontology-3/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <http://schema.org/> .
@prefix ontology: <http://www.narrative-universes.org/ontology#> .
@base <http://www.semanticweb.org/aless/ontologies/2026/0/untitled-ontology-3/> .

<http://www.narrative-universes.org/ontology> rdf:type owl:Ontology ;
                                               owl:versionIRI <http://www.narrative-universes.org/ontology/1.0> ;
                                               dc:creator "Alessandro Olivero" ,
                                                          "Giovanni Grillo" ;
                                               dc:date "2026-01-10T00:00:00"^^xsd:dateTime ;
                                               dc:description "An ontology for representing crossmedia narrative universes including characters, locations, objects, works, and narrative roles"@en ;
                                               dc:title "Ontologia degli universi narrativi"@it ;
                                               vann:preferredNamespacePrefix "\"nuo\"" ;
                                               vann:preferredNamespaceUri "\"http://www.narrative-universes.org/ontology#\"" ;
                                               rdfs:label "Narrative Universes Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.narrative-universes.org/ontology#adaptationOf
ontology:adaptationOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain ontology:NarrativeWork ;
                      rdfs:range ontology:NarrativeWork ;
                      rdfs:comment "Collega un'opera (tipicamente film) all'opera originale (libro) da cui è adattata"@it ;
                      rdfs:label "adattamento di"@it .


###  http://www.narrative-universes.org/ontology#alliedWith
ontology:alliedWith rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdf:type owl:SymmetricProperty ;
                    rdfs:domain ontology:Character ;
                    rdfs:range ontology:Character ;
                    rdfs:comment "Collega due personaggi che sono alleati o compagni"@it ;
                    rdfs:label "alleato con"@it .


###  http://www.narrative-universes.org/ontology#appearsIn
ontology:appearsIn rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf ontology:features ;
                   rdfs:domain ontology:Character ;
                   rdfs:range ontology:NarrativeWork ;
                   rdfs:comment "Collega un personaggio a un'opera narrativa in cui appare"@it ;
                   rdfs:label "appare in"@it .


###  http://www.narrative-universes.org/ontology#belongsToUniverse
ontology:belongsToUniverse rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain owl:Thing ;
                           rdfs:range ontology:NarrativeUniverse ;
                           rdfs:comment "Collega un'entità (personaggio, luogo, oggetto, creatura) all'universo narrativo a cui appartiene"@it ;
                           rdfs:label "appartiene a universo"@it .


###  http://www.narrative-universes.org/ontology#enemyOf
ontology:enemyOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:SymmetricProperty ;
                 rdfs:domain ontology:Character ;
                 rdfs:range ontology:Character ;
                 rdfs:comment "Collega due personaggi che sono nemici dichiarati"@it ;
                 rdfs:label "nemico di"@it .


###  http://www.narrative-universes.org/ontology#features
ontology:features rdf:type owl:ObjectProperty ;
                  rdfs:domain ontology:NarrativeWork ;
                  rdfs:range ontology:Character ;
                  rdfs:comment "Indica quali personaggi appaiono in un'opera narrativa"@it ;
                  rdfs:label "presente"@it .


###  http://www.narrative-universes.org/ontology#grantsAbility
ontology:grantsAbility rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:range ontology:Ability ;
                       rdfs:comment "Collega un artefatto di potere all'abilità che concede"@it ;
                       rdfs:label "concede abilità"@it .


###  http://www.narrative-universes.org/ontology#hasAbility
ontology:hasAbility rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain ontology:Character ;
                    rdfs:range ontology:Ability ;
                    rdfs:comment "Collega un personaggio a un'abilità che possiede naturalmente (senza artefatti)"@it ;
                    rdfs:label "ha abilità"@it .


###  http://www.narrative-universes.org/ontology#hasMainProtagonist
ontology:hasMainProtagonist rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf ontology:features ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain ontology:NarrativeWork ;
                            rdfs:range ontology:Protagonist ;
                            rdfs:comment "Collega un'opera al suo protagonista principale (uno solo per opera)"@it ;
                            rdfs:label "ha protagonista principale"@it .


###  http://www.narrative-universes.org/ontology#hasMember
ontology:hasMember rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf ontology:memberOf ;
                   rdfs:domain ontology:Organization ;
                   rdfs:range ontology:Character ;
                   rdfs:comment "Indica i membri di un'organizzazione"@it .


###  http://www.narrative-universes.org/ontology#hasOriginType
ontology:hasOriginType rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain ontology:HybridCharacter ;
                       rdfs:range ontology:OriginType ;
                       rdfs:comment "Collega un personaggio ibrido ai suoi tipi di origine (es: umano + divino)"@it ;
                       rdfs:label "ha tipo di origine"@it .


###  http://www.narrative-universes.org/ontology#hasPowerTypeEnum
ontology:hasPowerTypeEnum rdf:type owl:ObjectProperty ;
                          rdfs:domain ontology:Object ;
                          rdfs:range ontology:PowerType ;
                          rdfs:comment "Collega un oggetto magico al suo tipo di potere tramite la classe enumerata PowerType"@it ;
                          rdfs:label "ha tipo di potere"@it .


###  http://www.narrative-universes.org/ontology#hosts
ontology:hosts rdf:type owl:ObjectProperty ;
               owl:inverseOf ontology:locatedAt ;
               rdfs:domain ontology:Location ;
               rdfs:range ontology:Organization ;
               rdfs:comment "Indica quali organizzazioni si trovano in un luogo"@it ;
               rdfs:label "ospita"@it .


###  http://www.narrative-universes.org/ontology#indirectlyMentors
ontology:indirectlyMentors rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain ontology:Character ;
                           rdfs:range ontology:Character ;
                           owl:propertyChainAxiom ( ontology:mentors
                                                    ontology:alliedWith
                                                  ) ;
                           rdfs:comment "Indica una relazione di mentoring indiretta attraverso alleanze"@it ;
                           rdfs:label "fa da mentore indirettamente"@it .


###  http://www.narrative-universes.org/ontology#indirectlySetIn
ontology:indirectlySetIn rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         owl:propertyChainAxiom ( ontology:partOf
                                                ) ,
                                                ( ontology:setIn
                                                ) ;
                         rdfs:comment "Usata per rappresentare legami indiretti tra entità narrative e ambientazioni"@it ;
                         rdfs:label "Impostato indirettamente"@it .


###  http://www.narrative-universes.org/ontology#locatedAt
ontology:locatedAt rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain ontology:Organization ;
                   rdfs:range ontology:Location ;
                   rdfs:comment "Collega un'organizzazione al luogo fisico dove si trova"@it ;
                   rdfs:label "situato in"@it .


###  http://www.narrative-universes.org/ontology#memberOf
ontology:memberOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain ontology:Character ;
                  rdfs:range ontology:Organization ;
                  rdfs:comment "Collega un personaggio a un'organizzazione di cui fa parte"@it ;
                  rdfs:label "membro di"@it .


###  http://www.narrative-universes.org/ontology#mentors
ontology:mentors rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain ontology:Character ;
                 rdfs:range ontology:Character ;
                 rdfs:comment "Collega un personaggio mentore al personaggio che guida e insegna"@it ;
                 rdfs:label "fa da mentore a"@it .


###  http://www.narrative-universes.org/ontology#partOf
ontology:partOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain ontology:Location ;
                rdfs:range ontology:Location ;
                rdfs:comment "Collega un luogo a un luogo più grande che lo contiene"@it ;
                rdfs:label "parte di"@it .


###  http://www.narrative-universes.org/ontology#possesses
ontology:possesses rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain ontology:Character ;
                   rdfs:range ontology:Object ;
                   rdfs:comment "Collega un personaggio a un oggetto che possiede o controlla"@it ;
                   rdfs:label "possiede"@it .


###  http://www.narrative-universes.org/ontology#prequelOf
ontology:prequelOf rdf:type owl:ObjectProperty ;
                   owl:inverseOf ontology:sequelOf ;
                   rdfs:comment "Indica che un'opera narrativa è il prequel (storia precedente) di un'altra opera"@it ;
                   rdfs:label "prequel di"@it .


###  http://www.narrative-universes.org/ontology#sequelOf
ontology:sequelOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdf:type owl:TransitiveProperty ;
                  rdfs:domain ontology:NarrativeWork ;
                  rdfs:range ontology:NarrativeWork ;
                  rdfs:comment "Collega un'opera all'opera precedente di cui è il seguito"@it ;
                  rdfs:label "seguito di"@it .


###  http://www.narrative-universes.org/ontology#setIn
ontology:setIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain ontology:NarrativeWork ;
               rdfs:range ontology:Location ;
               rdfs:comment "Collega un'opera narrativa al luogo in cui è ambientata"@it ;
               rdfs:label "ambientato in"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#hasSeasons
<http://www.co-ode.org/ontologies/ont.owl#hasSeasons> rdf:type owl:DatatypeProperty ;
                                                      rdfs:domain ontology:NarrativeWork ;
                                                      rdfs:range xsd:integer ;
                                                      rdfs:comment "Il numero di stagioni che una serie televisiva possiede"@it ;
                                                      rdfs:label "ha stagioni"@it .


###  http://www.narrative-universes.org/ontology#birthYear
ontology:birthYear rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain ontology:Character ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "L'anno di nascita di un personaggio"@it ;
                   rdfs:label "anno di nascita"@it .


###  http://www.narrative-universes.org/ontology#canBeDestroyed
ontology:canBeDestroyed rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:range xsd:boolean ;
                        rdfs:comment "Indica se un artefatto può essere distrutto"@it ;
                        rdfs:label "può essere distrutto"@it .


###  http://www.narrative-universes.org/ontology#description
ontology:description rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Una descrizione testuale dell'entità"@it ;
                     rdfs:label "descrizione"@it .


###  http://www.narrative-universes.org/ontology#hasNarrativeFunction
ontology:hasNarrativeFunction rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf owl:topDataProperty ;
                              rdfs:domain ontology:Location ;
                              rdfs:range xsd:string ;
                              rdfs:comment "La funzione narrativa di un elemento (es: Transizione, Rifugio, Conflitto)"@it ;
                              rdfs:label "ha funzione narrativa"@it .


###  http://www.narrative-universes.org/ontology#isOngoing
ontology:isOngoing rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain ontology:TVSeries ;
                   rdfs:range xsd:boolean ;
                   rdfs:comment "Indica se una serie TV è ancora in produzione"@it ;
                   rdfs:label "in corso"@it .


###  http://www.narrative-universes.org/ontology#numberOfPages
ontology:numberOfPages rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain ontology:Book ;
                       rdfs:range xsd:integer ;
                       rdfs:comment "Il numero di pagine di un libro"@it ;
                       rdfs:label "numero di pagine"@it .


###  http://www.narrative-universes.org/ontology#publicationYear
ontology:publicationYear rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:domain ontology:NarrativeWork ;
                         rdfs:range xsd:integer ;
                         rdfs:comment "L'anno in cui un'opera è stata pubblicata"@it ;
                         rdfs:label "anno di pubblicazione"@it .


###  http://www.narrative-universes.org/ontology#rating
ontology:rating rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain ontology:NarrativeWork ;
                rdfs:range xsd:float ;
                rdfs:comment "La valutazione media di un'opera su scala 0-10"@it ;
                rdfs:label "valutazione"@it .


###  http://www.narrative-universes.org/ontology#runtime
ontology:runtime rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain ontology:NarrativeWork ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "La durata in minuti di un film o episodio"@it ;
                 rdfs:label "durata"@it .


###  http://www.narrative-universes.org/ontology#title
ontology:title rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain ontology:NarrativeWork ;
               rdfs:range xsd:string ;
               rdfs:comment "Il titolo di un'opera narrativa"@it ;
               rdfs:label "titolo"@it .


#################################################################
#    Classes
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#PowerArtifact
<http://www.co-ode.org/ontologies/ont.owl#PowerArtifact> rdf:type owl:Class ;
                                                         owl:equivalentClass [ owl:intersectionOf ( ontology:Object
                                                                                                    [ rdf:type owl:Restriction ;
                                                                                                      owl:onProperty ontology:grantsAbility ;
                                                                                                      owl:someValuesFrom ontology:Ability
                                                                                                    ]
                                                                                                  ) ;
                                                                               rdf:type owl:Class
                                                                             ] ;
                                                         rdfs:subClassOf ontology:Object ;
                                                         owl:disjointWith ontology:Weapon ;
                                                         rdfs:comment "Un oggetto magico di grande potere, non necessariamente un'arma da combattimento. Include anelli, mantelli, bacchette e altri artefatti leggendari"@it ;
                                                         rdfs:label "Artefatto di Potere"@it .


###  http://www.narrative-universes.org/ontology#Ability
ontology:Ability rdf:type owl:Class ;
                 rdfs:comment "Un'abilità o potere che può essere concesso da un artefatto o posseduto da un personaggio"@it ;
                 rdfs:label "Abilità"@it .


###  http://www.narrative-universes.org/ontology#Alleato
ontology:Alleato rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( ontology:Character
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty ontology:alliedWith ;
                                                              owl:someValuesFrom ontology:Character
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:comment "Amico o compagno che aiuta il protagonista"@it ;
                 rdfs:label "Alleato"@it .


###  http://www.narrative-universes.org/ontology#Antagonist
ontology:Antagonist rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( ontology:Character
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ontology:enemyOf ;
                                                                 owl:someValuesFrom ontology:Protagonist
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    owl:disjointWith ontology:Protagonist ;
                    rdfs:comment "Villain principale o forza oppositrice"@it ;
                    rdfs:label "Antagonista"@it .


###  http://www.narrative-universes.org/ontology#Book
ontology:Book rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( ontology:NarrativeWork
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty ontology:numberOfPages ;
                                                           owl:someValuesFrom xsd:integer
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf ontology:NarrativeWork ;
              rdfs:comment "Un'opera letteraria scritta, tipicamente un romanzo o una serie di libri"@it ;
              rdfs:label "Libro"@it ;
              skos:closeMatch schema:Book .


###  http://www.narrative-universes.org/ontology#Character
ontology:Character rdf:type owl:Class ;
                   rdfs:comment "Un personaggio fittizio che appare in una o più opere narrative"@it ;
                   rdfs:label "Personaggio"@it ;
                   skos:closeMatch schema:Person .


###  http://www.narrative-universes.org/ontology#DangerZone
ontology:DangerZone rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( ontology:Location
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ontology:hasNarrativeFunction ;
                                                                 owl:hasValue "Conflitto"
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf ontology:Location ;
                    rdfs:comment "Un luogo pericoloso o ostile"@it ;
                    rdfs:label "Zona Pericolosa"@it .


###  http://www.narrative-universes.org/ontology#DivineAbility
ontology:DivineAbility rdf:type owl:Class ;
                       rdfs:subClassOf ontology:Ability ;
                       owl:disjointWith ontology:MagicalAbility ;
                       rdfs:comment "Un'abilità di origine divina"@it ;
                       rdfs:label "Abilità Divina"@it .


###  http://www.narrative-universes.org/ontology#HumanCharacter
ontology:HumanCharacter rdf:type owl:Class ;
                        rdfs:subClassOf ontology:Character ;
                        owl:disjointWith ontology:HybridCharacter ,
                                         ontology:NonHumanCharacter ;
                        rdfs:comment "Un personaggio completamente umano"@it ;
                        rdfs:label "Personaggio Umano"@it .


###  http://www.narrative-universes.org/ontology#HybridCharacter
ontology:HybridCharacter rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( ontology:Character
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty ontology:hasOriginType ;
                                                                      owl:minCardinality "2"^^xsd:nonNegativeInteger
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf ontology:Character ;
                         owl:disjointWith ontology:NonHumanCharacter ;
                         rdfs:comment "Un personaggio di eredità mista (semidio, mezzo gigante, ecc.)"@it ;
                         rdfs:label "Personaggio Ibrido"@it .


###  http://www.narrative-universes.org/ontology#LiminalSpace
ontology:LiminalSpace rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( ontology:Location
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty ontology:hasNarrativeFunction ;
                                                                   owl:hasValue "Transizione"
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf ontology:Location ;
                      rdfs:comment "Una soglia o passaggio tra mondi o dimensioni"@it ;
                      rdfs:label "Spazio Liminale"@it .


###  http://www.narrative-universes.org/ontology#Location
ontology:Location rdf:type owl:Class ;
                  rdfs:comment "Un luogo o location all'interno di un universo narrativo"@it ;
                  rdfs:label "Luogo"@it ;
                  skos:closeMatch schema:Place .


###  http://www.narrative-universes.org/ontology#MagicalAbility
ontology:MagicalAbility rdf:type owl:Class ;
                        rdfs:subClassOf ontology:Ability ;
                        rdfs:comment "Un'abilità di natura magica"@it ;
                        rdfs:label "Abilità Magica"@it .


###  http://www.narrative-universes.org/ontology#Mentor
ontology:Mentor rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( ontology:Character
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty ontology:mentors ;
                                                             owl:someValuesFrom ontology:Character
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "Guida saggia che insegna e supporta il protagonista"@it ;
                rdfs:label "Mentore"@it .


###  http://www.narrative-universes.org/ontology#Movie
ontology:Movie rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( ontology:NarrativeWork
                                                          [ rdf:type owl:Class ;
                                                            owl:complementOf [ rdf:type owl:Restriction ;
                                                                               owl:onProperty ontology:isOngoing ;
                                                                               owl:hasValue "true"^^xsd:boolean
                                                                             ]
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty ontology:runtime ;
                                                            owl:someValuesFrom xsd:integer
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf ontology:NarrativeWork ;
               rdfs:comment "Un film cinematografico"@it ;
               rdfs:label "Film"@it ;
               skos:closeMatch schema:Movie .


###  http://www.narrative-universes.org/ontology#NarrativeUniverse
ontology:NarrativeUniverse rdf:type owl:Class ;
                           rdfs:comment "Un universo fittizio che si estende attraverso molteplici opere narrative su diversi media (libri, film, serie TV, videogiochi, teatro)"@it ;
                           rdfs:label "Universo Narrativo"@it .


###  http://www.narrative-universes.org/ontology#NarrativeWork
ontology:NarrativeWork rdf:type owl:Class ;
                       rdfs:comment "Una singola opera creativa (libro, film, episodio) che fa parte di un universo narrativo"@it ;
                       rdfs:label "Opera Narrativa"@it .


###  http://www.narrative-universes.org/ontology#NonHumanCharacter
ontology:NonHumanCharacter rdf:type owl:Class ;
                           rdfs:subClassOf ontology:Character ;
                           rdfs:comment "Un personaggio non umano (elfo, nano, satiro, ecc.)"@it ;
                           rdfs:label "Personaggio Non Umano"@it .


###  http://www.narrative-universes.org/ontology#Object
ontology:Object rdf:type owl:Class ;
                rdfs:comment "Un oggetto fisico o magico all'interno di un universo narrativo"@it ;
