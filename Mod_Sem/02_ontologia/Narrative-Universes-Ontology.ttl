###  http://www.narrative-universes.org/ontology#OriginType
ontology:OriginType rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:oneOf ( <http://www.co-ode.org/ontologies/ont.owl#ElvenOrigin>
                                                      <http://www.co-ode.org/ontologies/ont.owl#GiantOrigin>
                                                      <http://www.co-ode.org/ontologies/ont.owl#HumanOrigin>
                                                      <http://www.co-ode.org/ontologies/ont.owl#SpiritOrigin>
                                                      ontology:DivineOrigin
                                                    )
                                        ] ;
                    rdfs:label "Il tipo di origine di un personaggio ibrido (umano, divino, gigante, spirito)"@it ,
                               "Tipo di Origine"@it .


###  http://www.narrative-universes.org/ontology#PowerType
ontology:PowerType rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( <http://www.co-ode.org/ontologies/ont.owl#OrdinaryPower>
                                                     ontology:CorruptivePower
                                                     ontology:DivinePower
                                                     ontology:MagicalPower
                                                     ontology:SymbolicPower
                                                   )
                                       ] ;
                   rdfs:comment "Il tipo di potere di un artefatto magico. Può essere solo uno tra: Magico, Divino, Corruttivo o Simbolico"@it ;
                   rdfs:label "Tipo di Potere"@it .


###  http://www.narrative-universes.org/ontology#Protagonist
ontology:Protagonist rdf:type owl:Class ;
                     rdfs:subClassOf ontology:Character ;
                     rdfs:comment "Eroe principale o personaggio centrale della narrazione"@it ;
                     rdfs:label "Protagonista"@it .


###  http://www.narrative-universes.org/ontology#SafePlace
ontology:SafePlace rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( ontology:Location
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty ontology:hasNarrativeFunction ;
                                                                owl:hasValue "Rifugio"
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf ontology:Location ;
                   rdfs:comment "Un luogo considerato sicuro, un rifugio o santuario"@it ;
                   rdfs:label "Luogo Sicuro"@it .


###  http://www.narrative-universes.org/ontology#School
ontology:School rdf:type owl:Class ;
                rdfs:subClassOf ontology:Organization ;
                rdfs:comment "Un'istituzione educativa all'interno dell'universo narrativo"@it ;
                rdfs:label "Scuola"@it .


###  http://www.narrative-universes.org/ontology#TVSeries
ontology:TVSeries rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( ontology:NarrativeWork
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty <http://www.co-ode.org/ontologies/ont.owl#hasSeasons> ;
                                                               owl:someValuesFrom xsd:integer
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf ontology:NarrativeWork ;
                  rdfs:comment "Una serie televisiva con episodi multipli"@it ;
                  rdfs:label "Serie TV"@it .


###  http://www.narrative-universes.org/ontology#Weapon
ontology:Weapon rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( ontology:Object
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty ontology:hasPowerTypeEnum ;
                                                             owl:hasValue <http://www.co-ode.org/ontologies/ont.owl#OrdinaryPower>
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf ontology:Object ;
                rdfs:comment "Un'arma usata dai personaggi"@it ;
                rdfs:label "Arma"@it .


