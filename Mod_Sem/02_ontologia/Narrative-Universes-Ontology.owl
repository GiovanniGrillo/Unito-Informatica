############################
#   Object Properties
############################

# Object Property: ontology:adaptationOf (adattamento di)

AnnotationAssertion(rdfs:comment ontology:adaptationOf "Collega un'opera (tipicamente film) all'opera originale (libro) da cui è adattata"@it)
AnnotationAssertion(rdfs:label ontology:adaptationOf "adattamento di"@it)
SubObjectPropertyOf(ontology:adaptationOf owl:topObjectProperty)
ObjectPropertyDomain(ontology:adaptationOf ontology:NarrativeWork)
ObjectPropertyRange(ontology:adaptationOf ontology:NarrativeWork)

# Object Property: ontology:alliedWith (alleato con)

AnnotationAssertion(rdfs:comment ontology:alliedWith "Collega due personaggi che sono alleati o compagni"@it)
AnnotationAssertion(rdfs:label ontology:alliedWith "alleato con"@it)
SubObjectPropertyOf(ontology:alliedWith owl:topObjectProperty)
SymmetricObjectProperty(ontology:alliedWith)
ObjectPropertyDomain(ontology:alliedWith ontology:Character)
ObjectPropertyRange(ontology:alliedWith ontology:Character)

# Object Property: ontology:appearsIn (appare in)

AnnotationAssertion(rdfs:comment ontology:appearsIn "Collega un personaggio a un'opera narrativa in cui appare"@it)
AnnotationAssertion(rdfs:label ontology:appearsIn "appare in"@it)
SubObjectPropertyOf(ontology:appearsIn owl:topObjectProperty)
InverseObjectProperties(ontology:appearsIn ontology:features)
ObjectPropertyDomain(ontology:appearsIn ontology:Character)
ObjectPropertyRange(ontology:appearsIn ontology:NarrativeWork)

# Object Property: ontology:belongsToUniverse (appartiene all'universo)

AnnotationAssertion(rdfs:comment ontology:belongsToUniverse "Collega un'entità (personaggio, luogo, oggetto, creatura) all'universo narrativo a cui appartiene"@it)
AnnotationAssertion(rdfs:label ontology:belongsToUniverse "appartiene all'universo"@it)
SubObjectPropertyOf(ontology:belongsToUniverse owl:topObjectProperty)
ObjectPropertyDomain(ontology:belongsToUniverse owl:Thing)
ObjectPropertyRange(ontology:belongsToUniverse ontology:NarrativeUniverse)

# Object Property: ontology:createdBy (creato da)

AnnotationAssertion(rdfs:comment ontology:createdBy "Collega un oggetto o organizzazione al personaggio che lo ha creato"@it)
AnnotationAssertion(rdfs:label ontology:createdBy "creato da"@it)
SubObjectPropertyOf(ontology:createdBy owl:topObjectProperty)
ObjectPropertyDomain(ontology:createdBy owl:Thing)
ObjectPropertyRange(ontology:createdBy ontology:Character)

# Object Property: ontology:destroyedBy (distrutto da)

AnnotationAssertion(rdfs:comment ontology:destroyedBy "Collega un oggetto al personaggio che lo ha distrutto"@it)
AnnotationAssertion(rdfs:label ontology:destroyedBy "distrutto da"@it)
SubObjectPropertyOf(ontology:destroyedBy owl:topObjectProperty)
ObjectPropertyDomain(ontology:destroyedBy ontology:Object)
ObjectPropertyRange(ontology:destroyedBy ontology:Character)

# Object Property: ontology:enemyOf (nemico di)

AnnotationAssertion(rdfs:comment ontology:enemyOf "Collega due personaggi che sono nemici dichiarati"@it)
AnnotationAssertion(rdfs:label ontology:enemyOf "nemico di"@it)
SubObjectPropertyOf(ontology:enemyOf owl:topObjectProperty)
SymmetricObjectProperty(ontology:enemyOf)
ObjectPropertyDomain(ontology:enemyOf ontology:Character)
ObjectPropertyRange(ontology:enemyOf ontology:Character)

# Object Property: ontology:features (presenta)

AnnotationAssertion(rdfs:comment ontology:features "Indica quali personaggi appaiono in un'opera narrativa"@it)
AnnotationAssertion(rdfs:label ontology:features "presenta"@it)
ObjectPropertyDomain(ontology:features ontology:NarrativeWork)
ObjectPropertyRange(ontology:features ontology:Character)

# Object Property: ontology:grantsAbility (concede abilità)

AnnotationAssertion(rdfs:comment ontology:grantsAbility "Collega un artefatto di potere all'abilità che concede"@it)
AnnotationAssertion(rdfs:label ontology:grantsAbility "concede abilità"@it)
SubObjectPropertyOf(ontology:grantsAbility owl:topObjectProperty)
ObjectPropertyRange(ontology:grantsAbility ontology:Ability)

# Object Property: ontology:hasAbility (ha abilità)

AnnotationAssertion(rdfs:comment ontology:hasAbility "Collega un personaggio a un'abilità che possiede naturalmente (senza artefatti)"@it)
AnnotationAssertion(rdfs:label ontology:hasAbility "ha abilità"@it)
SubObjectPropertyOf(ontology:hasAbility owl:topObjectProperty)
ObjectPropertyDomain(ontology:hasAbility ontology:Character)
ObjectPropertyRange(ontology:hasAbility ontology:Ability)

# Object Property: ontology:hasMember (ontology:hasMember)

AnnotationAssertion(rdfs:comment ontology:hasMember "Indica i membri di un'organizzazione"@it)
SubObjectPropertyOf(ontology:hasMember owl:topObjectProperty)
InverseObjectProperties(ontology:hasMember ontology:memberOf)
ObjectPropertyDomain(ontology:hasMember ontology:Organization)
ObjectPropertyRange(ontology:hasMember ontology:Character)

# Object Property: ontology:hasOriginType (ha tipo di origine)

AnnotationAssertion(rdfs:comment ontology:hasOriginType "Collega un personaggio ibrido ai suoi tipi di origine (es: umano + divino)"@it)
AnnotationAssertion(rdfs:label ontology:hasOriginType "ha tipo di origine"@it)
SubObjectPropertyOf(ontology:hasOriginType owl:topObjectProperty)
ObjectPropertyDomain(ontology:hasOriginType ontology:HybridCharacter)
ObjectPropertyRange(ontology:hasOriginType ontology:OriginType)

# Object Property: ontology:hasRole (ha ruolo)

AnnotationAssertion(rdfs:comment ontology:hasRole "Collega un personaggio al suo ruolo narrativo"@it)
AnnotationAssertion(rdfs:label ontology:hasRole "ha ruolo"@it)
SubObjectPropertyOf(ontology:hasRole owl:topObjectProperty)
ObjectPropertyDomain(ontology:hasRole ontology:Character)
ObjectPropertyRange(ontology:hasRole ontology:NarrativeRole)

# Object Property: ontology:indirectlyMentors (fa da mentore indirettamente)

AnnotationAssertion(rdfs:comment ontology:indirectlyMentors "Indica una relazione di mentoring indiretta attraverso alleanze"@it)
AnnotationAssertion(rdfs:label ontology:indirectlyMentors "fa da mentore indirettamente"@it)
SubObjectPropertyOf(ontology:indirectlyMentors owl:topObjectProperty)
ObjectPropertyDomain(ontology:indirectlyMentors ontology:Character)
ObjectPropertyRange(ontology:indirectlyMentors ontology:Character)

# Object Property: ontology:indirectlySetIn (ontology:indirectlySetIn)

SubObjectPropertyOf(ontology:indirectlySetIn owl:topObjectProperty)

# Object Property: ontology:influences (influenza)

AnnotationAssertion(rdfs:comment ontology:influences "Collega un personaggio, oggetto o organizzazione a un evento narrativo che influenza significativamente"@it)
AnnotationAssertion(rdfs:label ontology:influences "influenza"@it)
SubObjectPropertyOf(ontology:influences owl:topObjectProperty)
ObjectPropertyDomain(ontology:influences owl:Thing)

# Object Property: ontology:locatedAt (situato in)

AnnotationAssertion(rdfs:comment ontology:locatedAt "Collega un'organizzazione al luogo fisico dove si trova"@it)
AnnotationAssertion(rdfs:label ontology:locatedAt "situato in"@it)
SubObjectPropertyOf(ontology:locatedAt owl:topObjectProperty)
InverseObjectProperties(ontology:locatedAt ontology:ospita)
ObjectPropertyDomain(ontology:locatedAt ontology:Organization)
ObjectPropertyRange(ontology:locatedAt ontology:Location)

# Object Property: ontology:memberOf (membro di)

AnnotationAssertion(rdfs:comment ontology:memberOf "Collega un personaggio a un'organizzazione di cui fa parte"@it)
AnnotationAssertion(rdfs:label ontology:memberOf "membro di"@it)
SubObjectPropertyOf(ontology:memberOf owl:topObjectProperty)
ObjectPropertyDomain(ontology:memberOf ontology:Character)
ObjectPropertyRange(ontology:memberOf ontology:Organization)

# Object Property: ontology:mentors (fa da mentore a)

AnnotationAssertion(rdfs:comment ontology:mentors "Collega un personaggio mentore al personaggio che guida e insegna"@it)
AnnotationAssertion(rdfs:label ontology:mentors "fa da mentore a"@it)
SubObjectPropertyOf(ontology:mentors owl:topObjectProperty)
ObjectPropertyDomain(ontology:mentors ontology:Character)
ObjectPropertyRange(ontology:mentors ontology:Character)

# Object Property: ontology:ospita (ospita)

AnnotationAssertion(rdfs:comment ontology:ospita "Indica quali organizzazioni si trovano in un luogo"@it)
AnnotationAssertion(rdfs:label ontology:ospita "ospita"@it)
ObjectPropertyDomain(ontology:ospita ontology:Location)
ObjectPropertyRange(ontology:ospita ontology:Organization)

# Object Property: ontology:partOf (parte di)

AnnotationAssertion(rdfs:comment ontology:partOf "Collega un luogo a un luogo più grande che lo contiene"@it)
AnnotationAssertion(rdfs:label ontology:partOf "parte di"@it)
SubObjectPropertyOf(ontology:partOf owl:topObjectProperty)
TransitiveObjectProperty(ontology:partOf)
ObjectPropertyDomain(ontology:partOf ontology:Location)
ObjectPropertyRange(ontology:partOf ontology:Location)

# Object Property: ontology:possesses (possiede)

AnnotationAssertion(rdfs:comment ontology:possesses "Collega un personaggio a un oggetto che possiede o controlla"@it)
AnnotationAssertion(rdfs:label ontology:possesses "possiede"@it)
SubObjectPropertyOf(ontology:possesses owl:topObjectProperty)
ObjectPropertyDomain(ontology:possesses ontology:Character)
ObjectPropertyRange(ontology:possesses ontology:Object)

# Object Property: ontology:prequelOf (ontology:prequelOf)

InverseObjectProperties(ontology:prequelOf ontology:sequelOf)

# Object Property: ontology:sequelOf (seguito di)

AnnotationAssertion(rdfs:comment ontology:sequelOf "Collega un'opera all'opera precedente di cui è il seguito"@it)
AnnotationAssertion(rdfs:label ontology:sequelOf "seguito di"@it)
SubObjectPropertyOf(ontology:sequelOf owl:topObjectProperty)
TransitiveObjectProperty(ontology:sequelOf)
ObjectPropertyDomain(ontology:sequelOf ontology:NarrativeWork)
ObjectPropertyRange(ontology:sequelOf ontology:NarrativeWork)

# Object Property: ontology:setIn (ambientato in)

AnnotationAssertion(rdfs:comment ontology:setIn "Collega un'opera narrativa al luogo in cui è ambientata"@it)
AnnotationAssertion(rdfs:label ontology:setIn "ambientato in"@it)
SubObjectPropertyOf(ontology:setIn owl:topObjectProperty)
ObjectPropertyDomain(ontology:setIn ontology:NarrativeWork)
ObjectPropertyRange(ontology:setIn ontology:Location)


