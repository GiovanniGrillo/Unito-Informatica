AnnotationAssertion(rdfs:comment ontology:description "Una descrizione testuale dell'entità"@it)
AnnotationAssertion(rdfs:label ontology:description "descrizione"@it)
SubDataPropertyOf(ontology:description owl:topDataProperty)
DataPropertyRange(ontology:description xsd:string)

# Data Property: ontology:hasNarrativeFunction (ha funzione narrativa)

AnnotationAssertion(rdfs:comment ontology:hasNarrativeFunction "La funzione narrativa di un elemento (es: Transizione, Rifugio, Conflitto)"@it)
AnnotationAssertion(rdfs:label ontology:hasNarrativeFunction "ha funzione narrativa"@it)
SubDataPropertyOf(ontology:hasNarrativeFunction owl:topDataProperty)
DataPropertyDomain(ontology:hasNarrativeFunction ontology:Location)
DataPropertyRange(ontology:hasNarrativeFunction xsd:string)

# Data Property: ontology:hasPowerType (ha tipo di potere)

AnnotationAssertion(rdfs:comment ontology:hasPowerType "Il tipo di potere di un artefatto (Magical, Divine, Corruptive, Symbolic)"@it)
AnnotationAssertion(rdfs:label ontology:hasPowerType "ha tipo di potere"@it)
SubDataPropertyOf(ontology:hasPowerType owl:topDataProperty)
DataPropertyRange(ontology:hasPowerType xsd:string)

# Data Property: ontology:isOngoing (in corso)

AnnotationAssertion(rdfs:comment ontology:isOngoing "Indica se una serie TV è ancora in produzione"@it)
AnnotationAssertion(rdfs:label ontology:isOngoing "in corso"@it)
SubDataPropertyOf(ontology:isOngoing owl:topDataProperty)
DataPropertyDomain(ontology:isOngoing ontology:TVSeries)
DataPropertyRange(ontology:isOngoing xsd:boolean)

# Data Property: ontology:numberOfPages (numero di pagine)

AnnotationAssertion(rdfs:comment ontology:numberOfPages "Il numero di pagine di un libro"@it)
AnnotationAssertion(rdfs:label ontology:numberOfPages "numero di pagine"@it)
SubDataPropertyOf(ontology:numberOfPages owl:topDataProperty)
DataPropertyDomain(ontology:numberOfPages ontology:Book)
DataPropertyRange(ontology:numberOfPages xsd:integer)

# Data Property: ontology:publicationDate (data di pubblicazione)

AnnotationAssertion(rdfs:comment ontology:publicationDate "La data esatta di pubblicazione di un'opera"@it)
AnnotationAssertion(rdfs:label ontology:publicationDate "data di pubblicazione"@it)
SubDataPropertyOf(ontology:publicationDate owl:topDataProperty)
DataPropertyDomain(ontology:publicationDate ontology:NarrativeWork)
DataPropertyRange(ontology:publicationDate xsd:dateTime)

# Data Property: ontology:publicationYear (anno di pubblicazione)

AnnotationAssertion(rdfs:comment ontology:publicationYear "L'anno in cui un'opera è stata pubblicata"@it)
AnnotationAssertion(rdfs:label ontology:publicationYear "anno di pubblicazione"@it)
SubDataPropertyOf(ontology:publicationYear owl:topDataProperty)
DataPropertyDomain(ontology:publicationYear ontology:NarrativeWork)
DataPropertyRange(ontology:publicationYear xsd:integer)

# Data Property: ontology:rating (valutazione)

AnnotationAssertion(rdfs:comment ontology:rating "La valutazione media di un'opera su scala 0-10"@it)
AnnotationAssertion(rdfs:label ontology:rating "valutazione"@it)
SubDataPropertyOf(ontology:rating owl:topDataProperty)
DataPropertyDomain(ontology:rating ontology:NarrativeWork)
DataPropertyRange(ontology:rating xsd:float)

# Data Property: ontology:runtime (durata)

AnnotationAssertion(rdfs:comment ontology:runtime "La durata in minuti di un film o episodio"@it)
AnnotationAssertion(rdfs:label ontology:runtime "durata"@it)
SubDataPropertyOf(ontology:runtime owl:topDataProperty)
DataPropertyDomain(ontology:runtime ontology:Movie)
DataPropertyDomain(ontology:runtime ontology:TVSeries)
DataPropertyRange(ontology:runtime xsd:integer)

# Data Property: ontology:title (titolo)

AnnotationAssertion(rdfs:comment ontology:title "Il titolo di un'opera narrativa"@it)
AnnotationAssertion(rdfs:label ontology:title "titolo"@it)
SubDataPropertyOf(ontology:title owl:topDataProperty)
DataPropertyDomain(ontology:title ontology:NarrativeWork)
DataPropertyRange(ontology:title xsd:string)



############################
#   Classes
############################

# Class: ontology:Ability (Abilità)

AnnotationAssertion(rdfs:comment ontology:Ability "Un'abilità o potere che può essere concesso da un artefatto o posseduto da un personaggio"@it)
AnnotationAssertion(rdfs:label ontology:Ability "Abilità"@it)

# Class: ontology:Ally (Alleato)

AnnotationAssertion(rdfs:comment ontology:Ally "Amico o compagno che aiuta il protagonista"@it)
AnnotationAssertion(rdfs:label ontology:Ally "Alleato"@it)
EquivalentClasses(ontology:Ally ObjectIntersectionOf(ontology:Character ObjectSomeValuesFrom(ontology:alliedWith ontology:Character)))
SubClassOf(ontology:Ally ontology:NarrativeRole)

# Class: ontology:Antagonist (Antagonista)

AnnotationAssertion(rdfs:comment ontology:Antagonist "Villain principale o forza oppositrice"@it)
AnnotationAssertion(rdfs:label ontology:Antagonist "Antagonista"@it)
EquivalentClasses(ontology:Antagonist ObjectIntersectionOf(ontology:Character ObjectSomeValuesFrom(ontology:enemyOf ontology:Protagonist)))
SubClassOf(ontology:Antagonist ontology:NarrativeRole)

# Class: ontology:Book (Libro)

AnnotationAssertion(rdfs:comment ontology:Book "Un'opera letteraria scritta, tipicamente un romanzo o una serie di libri"@it)
AnnotationAssertion(rdfs:label ontology:Book "Libro"@it)
EquivalentClasses(ontology:Book ObjectIntersectionOf(ontology:NarrativeWork DataSomeValuesFrom(ontology:numberOfPages xsd:integer)))
SubClassOf(ontology:Book ontology:NarrativeWork)

# Class: ontology:Character (Personaggio)

AnnotationAssertion(rdfs:comment ontology:Character "Un personaggio fittizio che appare in una o più opere narrative"@it)
AnnotationAssertion(rdfs:label ontology:Character "Personaggio"@it)

# Class: ontology:DangerZone (Zona Pericolosa)

AnnotationAssertion(rdfs:comment ontology:DangerZone "Un luogo pericoloso o ostile"@it)
AnnotationAssertion(rdfs:label ontology:DangerZone "Zona Pericolosa"@it)
EquivalentClasses(ontology:DangerZone ObjectIntersectionOf(ontology:Location DataHasValue(ontology:hasNarrativeFunction "Conflitto")))
SubClassOf(ontology:DangerZone ontology:Location)

# Class: ontology:Divine (Divino)

AnnotationAssertion(rdfs:label ontology:Divine "Divino"@it)
SubClassOf(ontology:Divine ontology:OriginType)

# Class: ontology:DivineAbility (Abilità Divina)

AnnotationAssertion(rdfs:comment ontology:DivineAbility "Un'abilità di origine divina"@it)
AnnotationAssertion(rdfs:label ontology:DivineAbility "Abilità Divina"@it)
SubClassOf(ontology:DivineAbility ontology:Ability)

# Class: ontology:Elven (Elfico)

AnnotationAssertion(rdfs:label ontology:Elven "Elfico"@it)
SubClassOf(ontology:Elven ontology:OriginType)

# Class: ontology:Giant (Gigante)

AnnotationAssertion(rdfs:label ontology:Giant "Gigante"@it)
SubClassOf(ontology:Giant ontology:OriginType)

# Class: ontology:Human (Umano)

AnnotationAssertion(rdfs:label ontology:Human "Umano"@it)
SubClassOf(ontology:Human ontology:OriginType)

# Class: ontology:HumanCharacter (Personaggio Umano)

AnnotationAssertion(rdfs:comment ontology:HumanCharacter "Un personaggio completamente umano"@it)
AnnotationAssertion(rdfs:label ontology:HumanCharacter "Personaggio Umano"@it)
SubClassOf(ontology:HumanCharacter ontology:Character)

# Class: ontology:HybridCharacter (Personaggio Ibrido)

AnnotationAssertion(rdfs:comment ontology:HybridCharacter "Un personaggio di eredità mista (semidio, mezzo gigante, ecc.)"@it)
AnnotationAssertion(rdfs:label ontology:HybridCharacter "Personaggio Ibrido"@it)
EquivalentClasses(ontology:HybridCharacter ObjectIntersectionOf(ontology:Character ObjectMinCardinality(2 ontology:hasOriginType)))
SubClassOf(ontology:HybridCharacter ontology:Character)

# Class: ontology:LiminalSpace (Spazio Liminale)

AnnotationAssertion(rdfs:comment ontology:LiminalSpace "Una soglia o passaggio tra mondi o dimensioni"@it)
